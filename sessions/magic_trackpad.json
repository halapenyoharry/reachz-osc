{
  "createdWith": "Open Stage Control",
  "version": "1.29.8",
  "type": "session",
  "content": {
    "type": "root",
    "lock": false,
    "id": "root",
    "visible": true,
    "interaction": true,
    "width": "auto",
    "height": "auto",
    "colorText": "#ffffff",
    "colorWidget": "#1a1a2e",
    "colorBg": "#0d1117",
    "layout": "default",
    "contain": true,
    "scroll": true,
    "innerPadding": true,
    "widgets": [
      {
        "type": "multixy",
        "top": 20,
        "left": 20,
        "id": "multixy",
        "visible": true,
        "interaction": true,
        "comments": "Multi-touch trackpad - use two fingers for gestures",
        "width": 480,
        "height": 320,
        "colorText": "#ffffff",
        "colorWidget": "#161b22",
        "colorStroke": "#58a6ff",
        "colorFill": "#58a6ff",
        "alphaFillOff": 0.05,
        "alphaFillOn": 0.3,
        "borderRadius": 16,
        "padding": 12,
        "pointSize": 24,
        "ephemeral": false,
        "points": 2,
        "rangeX": { "min": 0, "max": 1 },
        "rangeY": { "min": 0, "max": 1 },
        "decimals": 4,
        "address": "/multixy",
        "target": "127.0.0.1:9000",
        "sensitivity": 1,
        "snap": false,
        "spring": false
      },
      {
        "type": "button",
        "top": 360,
        "left": 20,
        "id": "two_finger_tap",
        "visible": true,
        "interaction": true,
        "comments": "Two-finger tap = right click",
        "width": 150,
        "height": 50,
        "colorText": "#ffffff",
        "colorWidget": "#161b22",
        "colorStroke": "#58a6ff",
        "colorFill": "#58a6ff",
        "alphaFillOff": 0.1,
        "alphaFillOn": 0.5,
        "borderRadius": 8,
        "label": "2-TAP (Right)",
        "on": 1,
        "off": 0,
        "mode": "tap",
        "address": "/multixy/tap",
        "target": "127.0.0.1:9000"
      },
      {
        "type": "button",
        "top": 360,
        "left": 180,
        "id": "tap",
        "visible": true,
        "interaction": true,
        "comments": "Single click",
        "width": 150,
        "height": 50,
        "colorText": "#ffffff",
        "colorWidget": "#161b22",
        "colorStroke": "#58a6ff",
        "colorFill": "#58a6ff",
        "alphaFillOff": 0.1,
        "alphaFillOn": 0.5,
        "borderRadius": 8,
        "label": "TAP (Left)",
        "on": 1,
        "off": 0,
        "mode": "tap",
        "address": "/tap",
        "target": "127.0.0.1:9000"
      },
      {
        "type": "text",
        "top": 360,
        "left": 350,
        "id": "instructions",
        "visible": true,
        "width": 150,
        "height": 50,
        "colorText": "#8b949e",
        "colorWidget": "transparent",
        "html": "<div style='font-size:11px;line-height:1.3'>2 fingers: scroll<br>pinch: zoom</div>",
        "vertical": true
      }
    ],
    "tabs": []
  }
}
